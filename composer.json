{
    "name": "drydev/php-wxwork-sdk",
    "description": "WeChat Work SDK for PHP/Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drydev\\WxWork\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drydev\\WxWork\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WxWork\\Laravel\\WxWorkServiceProvider"
            ],
            "aliases": {
                "WxWork": "WxWork\\Laravel\\Facades\\WxWork"
            }
        }
    }
}
